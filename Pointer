#include <stdio.h>

// func
int iFunc(){return 0;};
char cFunc(){return '0';};
double dFunc(){return 0.0;};

int main()
{
    
    // value
    int iValue = 0;
    char cValue = 0;
    double dValue = 0;
    
    // pointer
    int *iPointer = nullptr;
    char *cPointer = nullptr;
    double *dPointer = nullptr;
    
    iPointer = &iValue;
    cPointer = &cValue;
    dPointer = &dValue;
    
    // array
    int iArray[8] = {0,1,2,};
    char cArray[8] = {'0','1','2'};
    double dArray[8] = {0.0,1.0,2.0};
    
    // array pointer
    int (*pIArray)[8] = nullptr;          // array pointer
    char (*pCArray)[8] = nullptr;
    double (*pDArray)[8] = nullptr;
    
    pIArray = &iArray;
    pCArray = &cArray;
    pDArray = &dArray;
    
    // pointer array
    int *iArrPointer[8] = {iPointer};
    char *cArrPointer[8] = {cPointer};
    double *dArrPointer[8] = {dPointer};
    
    // func Pointer
    int (*pIFunc)() = nullptr;
    char (*pCFunc)() = nullptr;
    double (*pDFunc)() = nullptr;
    
    pIFunc = iFunc; // = &iFunc;
    pCFunc = cFunc; // = &cFunc;
    pDFunc = dFunc; // = &dFunc;
    
    // func array
    //int (iFArray[8])();
    //char (cFArray[8])();
    //double (dFArray[8])();
    
    // func pointer array
    int (*iPFArray[8])() = {pIFunc};
    char (*cPFArray[8])() = {pCFunc};
    double (*dPFArray[8])() = {pDFunc};
    
    
    printf("Hello World");
    return 0;
}
