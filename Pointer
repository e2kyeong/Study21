#include <stdio.h>



// func
int iFunc(){return 0;};
char cFunc(){return '0';};
double dFunc(){return 0.0;};

// pointer
int *gIPointer;
char *gCPointer;
double *gDPointer;

// func 
int *iPFunc(){
    return gIPointer;
};

char *cPFunf(){
    return gCPointer;
};

double *dPFunc(){
    return gDPointer;   
};

// return type is, array pointer 
int (*iPRetFunc())[8]{
    int (*iPArray)[8];
    return iPArray;
}

char (*cPRetFunc())[8]{
    char (*cPArray)[8];
    return cPArray;
}

double (*dPRetFunc())[8]{
    double (*dPArray)[8];
    return dPArray;
}

// param type is, array pointer
void iPParamFunc(int (*ptr)[8]){
    int (*_iArrPointer)[8] = ptr;
}

void cPParamFunc(char (*ptr)[8]){
    char (*cArrPointer)[8] = ptr;
}

void dPParamFunc(double (*ptr)[8]){
    double (*dArrPointer)[8] = ptr;
}

int main()
{
    
    // 'value'
    int iValue = 0;
    char cValue = 0;
    double dValue = 0;
    
    // 'pointer'
    int *iPointer = nullptr;
    char *cPointer = nullptr;
    double *dPointer = nullptr;
    
    iPointer = &iValue;
    cPointer = &cValue;
    dPointer = &dValue;
    
    // 'array'
    int iArray[8] = {0,1,2,};
    char cArray[8] = {'0','1','2'};
    double dArray[8] = {0.0,1.0,2.0};
    
    // array 'pointer'
    int (*pIArray)[8] = nullptr;          // array pointer
    char (*pCArray)[8] = nullptr;
    double (*pDArray)[8] = nullptr;
    
    pIArray = &iArray;
    pCArray = &cArray;
    pDArray = &dArray;
    
    // pointer 'array'
    int *iArrPointer[8] = {iPointer};
    char *cArrPointer[8] = {cPointer};
    double *dArrPointer[8] = {dPointer};
    
    // func 'Pointer'
    int (*pIFunc)() = nullptr;
    char (*pCFunc)() = nullptr;
    double (*pDFunc)() = nullptr;
    
    pIFunc = iFunc; // = &iFunc;
    pCFunc = cFunc; // = &cFunc;
    pDFunc = dFunc; // = &dFunc;
    
    // func array
    //int (iFArray[8])();
    //char (cFArray[8])();
    //double (dFArray[8])();
    
    // func pointer 'array'
    int (*iPFArray[8])() = {pIFunc};
    char (*cPFArray[8])() = {pCFunc};
    double (*dPFArray[8])() = {pDFunc};
    
    
    
    // return type is, array pointer 
    pIArray = iPRetFunc();
    pCArray = cPRetFunc();
    pDArray = dPRetFunc();
    
    iPParamFunc(pIArray);
    cPParamFunc(pCArray);
    dPParamFunc(pDArray);
    
    
    printf("Hello World");
    return 0;
}

