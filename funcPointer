#include <stdio.h>
#include <iostream>
using namespace std;
int a;
int aaa[10];
void add(int a, int b){
    cout<<a+b<<endl;
}
void sub(int a, int b){
    cout<<a-b<<endl;
    
}
void mul(int a, int b){
    cout<<a*b<<endl;
    
}
void diva(int a, int b){
    cout<<a/b<<endl;
    
}

int main()
{
    printf("Hello World\r\n");
    
    void (*pAdd)(int, int)  = &add;
    void (*ftable[4])(int, int) = {&add, &sub, &mul, &diva};
    for(void (*f)(int, int) : ftable){
        int a=2;
        int b=3;
        f(a,b);
    }
    return 0;
}
